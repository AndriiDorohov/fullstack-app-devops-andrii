# PostgreSQL configuration
POSTGRES_DB=fullstack_db
POSTGRES_USER=fullstack_user
POSTGRES_PASSWORD=fullstack_pass

# Backend configuration
BACKEND_PORT=3001

# Connection string used by the backend inside Docker
DATABASE_URL=postgres://fullstack_user:fullstack_pass@db:5432/fullstack_db

# Docker Hub username (used by docker-compose.prod.yml)
DOCKERHUB_USERNAME=your_dockerhub_login
